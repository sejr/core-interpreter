program
	int X1, X2, X3;
	int Y1, Y2, Y3;
begin
	X1 = 0;
	X2 = X1;
	X3 = X2;
	Y1 = 0;
	Y2 = Y1;
	Y3 = Y2;
	if [(X2==X2) && (Y2 ==   Y3)]then
		while [(X2 >= 0) && (X2 < 50)			] loop
			write X1, X2, X3;
			write Y1, Y2, Y3;
			X2 = X2 + 5	;	
			X1 = Y3 + 4;

			if [(Y3 > 0) || [(X3 > 0) && (X3 < 10)]] then
				while (Y3 >= 0) loop
					Y3 = X3 - 2;
					write Y3;
				end;
			else
				while (Y1 >= 0) loop
					Y1 = Y3 - X2;
					write Y1;
				end;
			end;		
		end;		
		if (X3==0) then
			Y3 = X2;
		else
			X3 = X2 - 1;
		end;
		write X2;
	end;
end